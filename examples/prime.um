சாற்று பகுக்கக்கூடிய = வினை(எண்) {
  க = 2
  விளைவு = பொய்
  மீண்டும் {
    எனில்(எண் % க == 0) {
      விளைவு = மெய்
    }
    க = க + 1
    நிறுத்து க >= எண் - 1
  }
  விளைவு
}

சாற்று பகா_எண்கள்  =  வினை(வரம்பு) {
  ஐ = 2
  மீண்டும் {
    இல்லைஎனில் (பகுக்கக்கூடிய(ஐ)) {
      அச்சு ஐ
    }
    ஐ = ஐ + 1
    நிறுத்து ஐ > வரம்பு
  }
}

